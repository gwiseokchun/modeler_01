        <th style="width: 10%">수정자</th>
        <th style="width: 10%">모델타입</th>
        <th style="width: 5%;">더보기</th>
      </tr>
    </thead>

    <tbody class="tableBody" style="visibility: hidden;">
      <% for (var i=0; i < data.length; i++) { %>
      <%  var rows = data[i]; %>  

      <tr style="background-color: #ffffff; border: 1px solid hsl(0 0% 87% / 1); ">
        <td><input type="checkbox"></td>
        <td style="display : none;"><%= rows.MODELID  %></td>
        <td ><%= rows.PROCESSID  %></td>
        <td ><%= rows.MODELNAME %></td>
        <td ><%= rows.MODELDESC %></td>
        <td style="padding-left: 20px;" ><%= rows.MODELDIAGRAM_CNT %></td>
        <td ><%= rows.UPDDTTM %></td>
        <td ><%= rows.INSUSER %></td>   
        <td ><%= rows.UPDUSER %></td>
        <td><%= rows.MODELCATID %></td>
        <td>
        <button class="context-menu-one btn btn-link" style="background-color: #ffffff;" 
                   id=<%= rows.MODELID %> > 
          <img src="/menu.svg" style="padding-right: 5px;" with="13", height="13">            
        </button> 
        </td>
      </tr>
      <tr>
        <td  ></td>
      </tr>
      <% } %>
